{
  "name": "huobi-chain-api",
  "version": "0.0.1",
  "description": "An API server for Huobi Chain",
  "main": "index.js",
  "bin": {
    "hermit-start-server": "npm start"
  },
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf lib",
    "build": "npm -s run clean && npm -s run generate && tsc --skipLibCheck",
    "generate": "dotenv -- npm -s run generate:nexus",
    "generate:docs": "graphql-markdown src/generated/schema.graphql > docs/schema.md",
    "generate:nexus": "ts-node  --transpile-only src/server/make-schema.ts",
    "generate:mysql-schemats": "dotenv -- ts-node src/scripts/generate-schema.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:server": "ts-node  --transpile-only src/server",
    "dev:sync": "cross-env DEBUG=sync:* ts-node --transpile-only src/sync",
    "sync": "node dist/sync",
    "release:alpha": "standard-version --prerelease alpha",
    "migrate": "ts-node --transpile-only src/scripts/migrate.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homura/huobi-chain-api.git"
  },
  "keywords": [
    "muta",
    "huobi-chain"
  ],
  "author": "homura.dev@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/homura/huobi-chain-api/issues"
  },
  "homepage": "https://github.com/homura/huobi-chain-api#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "hermit-purple-server": "^0.3.0-alpha.0",
    "http-proxy-middleware": "^1.0.3",
    "knex-on-duplicate-update": "^1.1.3",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "muta-sdk": "^0.8.0-alpha.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "dotenv-cli": "^3.1.0",
    "module-alias": "^2.2.2",
    "schemats": "^3.0.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
